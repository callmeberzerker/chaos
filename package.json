{
	"name": "chaos",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "svelte-kit dev",
		"prebuild": "test \"$CI\" = true && npx pnpm install -r --store=node_modules/.pnpm-store || echo skipping pnpm install",
		"build": "svelte-kit build",
		"postbuild": "node scripts/adapt-build",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@jackfranklin/rollup-plugin-markdown": "^0.3.0",
		"@sveltejs/adapter-node": "next",
		"@sveltejs/adapter-vercel": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^4.28.2",
		"@typescript-eslint/parser": "^4.28.2",
		"eslint": "^7.30.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.0",
		"mdsvex": "^0.9.8",
		"postcss": "^8.3.5",
		"postcss-load-config": "^3.1.0",
		"prettier": "~2.2.1",
		"prettier-plugin-svelte": "^2.3.1",
		"rehype-autolink-headings": "^5.1.0",
		"rehype-slug": "^4.0.1",
		"remark-abbr": "^1.4.1",
		"remark-github": "^10.1.0",
		"svelte": "^3.38.3",
		"svelte-check": "^2.2.0",
		"svelte-preprocess": "^4.7.3",
		"tslib": "^2.3.0",
		"typescript": "^4.3.5"
	},
	"dependencies": {
		"@cmdlucas/markdown-metadata": "^1.0.7",
		"dayjs": "^1.10.6",
		"js-yaml": "^4.1.0",
		"rehype-highlight": "^4.1.0",
		"rehype-stringify": "^8.0.0",
		"remark-frontmatter": "^3.0.0",
		"remark-gfm": "^1.0.0",
		"remark-parse": "^9.0.0",
		"remark-rehype": "^8.1.0",
		"remark-stringify": "^9.0.1",
		"svelte-preprocess": "^4.7.3",
		"unified": "^9.2.1"
	}
}